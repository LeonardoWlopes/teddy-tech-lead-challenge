{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"globalEnv": ["DEV"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": []
		},
		"lint": {
			"dependsOn": ["^lint"],
			"cache": false
		},
		"check-types": {
			"dependsOn": ["^check-types"],
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
